<% content_for :page_actions do %>
  <li><%= button_link_to t(:back_to_post_categories_list), admin_post_categories_url, :icon => 'icon-arrow-left' %></li>
<% end %>

<%#= render :partial => 'spree/admin/shared/contents_sub_menu' %>

<%= render "spree/shared/error_messages", :target => @post_category %>

<%= render :partial => 'spree/blogs/admin/shared/post_tabs', :locals => {:current => "Categories"} unless request.xhr? %>

<%= form_for([:admin, @post_category], :html => { :multipart => true }) do |f| %>
  <fieldset data-hook="new_upload">
    <legend align="center">New Catefory</legend>

    <%= render :partial => 'form', :locals => {:f => f} %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
